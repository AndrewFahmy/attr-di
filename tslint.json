{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "no-var-requires": false,
    "no-empty": false,
    "no-consecutive-blank-lines": false,
    "no-string-literal": false
  }
}